---
import Table from "../../components/Table.astro";
import { AIRFRYER_TIMES } from "../../data/airfryer_times";
import MainLayout from "../../layouts/MainLayout.astro";

const title = "Temps en airfryer";
const description =
  "Pàgina web que proporciona informació sobre la duració i temperatura de cuinar ingredients en airfryer";
---

